[core]
    editor = vim
[include]
    path = ~/.config/git/config.local
[push]
    default = simple
[rebase]
    autosquash = true
[alias]
    lg = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%H%C(reset) - %C(bold green)(%ad)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)'
[log]
    date = iso

